    "version": "0.1.0",
    "configurations": [
        {
            "name": "Java Remote",
            "jdkPath": "C:/Program Files/Java/jdk1.8.0_121/bin",
            "type": "java",
            "request": "attach",
            "stopOnEntry": true,
            "cwd": "${workspaceRoot}/myteam/src/main/java",
            "startupClass": "${relativeFile}",
            "sourcePath": ["${workspaceRoot}/myteam/src/main/java"],
            "classpath": ["${workspaceRoot}/myteam/target/classes"],
            "remoteHost": "localhost",
            "remotePort": 5005
        }
    ]
}