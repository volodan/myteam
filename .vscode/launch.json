{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.1.0",
    "configurations": [
        {
            "name": "Java Remote",
            "jdkPath": "/Library/Java/JavaVirtualMachines/jdk1.8.0_131.jdk/Contents/Home/bin",
            "type": "java",
            "request": "attach",
            "stopOnEntry": true,
            "cwd": "${workspaceRoot}/src/main/java",
            "startupClass": "${relativeFile}",
            "sourcePath": ["${workspaceRoot}/src/main/java"],
            "classpath": ["${workspaceRoot}/target/classes"],
            "remoteHost": "localhost",
            "remotePort": 5005
        }
    ]
}